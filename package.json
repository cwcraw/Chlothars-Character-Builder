{
  "name": "chlothars-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "migrate": "knex migrate:latest --knexfile ./model/knexfile.js",
    "newMigration": "knex migrate:make migrate_1.js --migrations-directory ./model/migrations",
    "seed": "yarn knex seed:run --knexfile=./model/knexfile.js",
    "FEStart": "vue-cli-service serve",
    "start": "node server/server.js",
    "BEStart": "yarn nodemon ./server/server.js",
    "heroku-postbuild": "vue-cli-service serve"
  },
  "dependencies": {
    "apollo-boost": "^0.4.8",
    "apollo-server": "^2.13.0",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "knex": "^0.21.1",
    "nodemon": "^2.0.3",
    "pg": "^8.1.0",
    "vue": "^2.6.11",
    "vue-apollo": "^3.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
